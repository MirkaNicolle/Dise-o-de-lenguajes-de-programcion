digraph {
	S1 [shape=doublecircle]
	S2 [shape=doublecircle]
	S2 -> S2 [label=b]
	S3 [shape=doublecircle]
	S3 -> S2 [label=b]
	S3 -> S3 [label=a]
	S4 [shape=doublecircle]
	S4 -> S2 [label=b]
	S4 -> S3 [label=a]
	S4 -> S4 [label=c]
	S3 -> S4 [label=c]
	S2 -> S3 [label=a]
	S2 -> S4 [label=c]
	S1 -> S2 [label=b]
	S1 -> S3 [label=a]
	S1 -> S4 [label=c]
}
